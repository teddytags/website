{
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/repl", "dest": "/index.html" },
    { "src": "/docs", "dest": "/index.html" },
    { "src": "/docs/.*", "dest": "/index.html" },
    { "src": "/blog", "dest": "/index.html" },
    { "src": "/blog/.*", "dest": "/index.html" },
    {
      "src": "/service-worker.js",
      "dest": "/service-worker.js",
      "headers": { "cache-control": "no-cache" }
    },
    { "src": "/(.+)", "dest": "/$1" }
  ]
}
